{
  "name": "dotstatsuite-config",
  "version": "3.0.0",
  "scripts": {
    "dist": "cross-env NODE_ENV=production babel --copy-files  src -d dist",
    "dist:run": "cross-env NODE_ENV=production DEBUG=config* node dist/server",
    "start:srv": "cross-env NODE_ENV=development DEBUG=config* nodemon --watch src/server --exec 'babel-node src/server'",
    "lint": "yarn lint:js",
    "lint:eslint": "eslint --ignore-pattern '!.eslintrc.js'",
    "lint:js": "yarn lint:eslint -- . ",
    "precommit": "lint-staged",
    "test": "cross-env NODE_ENV=test jest",
    "i18n:update": "node scripts/i18n-update.js --color"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "prettier --write",
      "git add",
      "yarn lint:eslint"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "env": "^0.0.2",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.12.4",
    "express": "4.16.2",
    "express-pino-logger": "^5.0.0",
    "fs-extra": "^7.0.1",
    "pino": "^6.6.1",
    "pino-multi-stream": "^5.1.1",
    "pino-pretty": "^4.2.1",
    "pino-stackdriver": "^2.1.1",
    "prettier": "^1.16.1",
    "ramda": "^0.26.1"
  },
  "jest": {
    "verbose": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/server/index.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/tests/**/*.test.js"
    ],
    "testEnvironment": "node"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-regenerator",
      "@babel/plugin-proposal-export-default-from",
      "@babel/plugin-syntax-dynamic-import",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-plugin-named-asset-import": "^0.3.0",
    "babel-plugin-ramda": "^2.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.3.1",
    "jest": "^24.0.0",
    "lint-staged": "^8.1.0",
    "nodemon": "^1.18.9"
  }
}
